{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "no-alert": 0,
    "linebreak-style": 0,
    "no-else-return": 0,
    "jsx-a11y/anchor-is-valid": 0,
    "react/jsx-filename-extension": 0,
    "arrow-parens": "off",
    "arrow-body-style": "off"
  },
  "env": {
    "browser": true
  }
}
